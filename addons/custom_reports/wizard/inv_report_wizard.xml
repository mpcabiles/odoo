<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="inv_report_wizard_view" model="ir.ui.view">
            <field name="name">inv.report.wizard.view</field>
            <field name="model">inv.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Sales Details">
                    <group colspan="2" col="2">
                        <field name="inv_start_date" />
                        <field name="inv_end_date" />
                    </group>
                    <footer>
                        <button name="get_inv_report" string="Print" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

</odoo>
